        <stop offset="0%" style="stop-color:rgb(50,105,122);stop-opacity:1"/>
        <stop offset="100%" style="stop-color:rgb(42,56,65);stop-opacity:1"/>
      </linearGradient>
      <linearGradient id="gray_gray" x1="0%" y1="0%" x2="0%" y2="100%">
