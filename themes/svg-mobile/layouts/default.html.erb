      <linearGradient id="blue_black" x1="0%" y1="0%" x2="0%" y2="100%">
        <stop offset="0%" style="stop-color:rgb(87,44,38);stop-opacity:1"/>
        <stop offset="60%" style="stop-color:rgb(00,00,00);stop-opacity:1"/>
      </linearGradient>
      <style type="text/css">
        text {
          fill: #ffffff;
          font-size: 1em;
        }
        a .button {
          fill: url(#green_green);
        }
        a:hover .button {
          fill:url(#gray_gray);
        }
      </style>
    </defs>
    <rect rx="0" ry="0" x="0" y="0" width="100" height="100" fill="url(#blue_black)" />
  </svg>

